{
  "name": "Jok",
  "description": "It's jok time :)",
  "version": "1.4",
  "default_locale": "en",
  "app": {
    "launch": {
      "web_url": "http://jok.ge/?source=chromeApp",
     "container":"tab"
    }
  },
  "icons": {
    "128": "JokLogo.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "content_scripts" : [{
    "matches" : [ "*"],
    "js" : ["script.js"]
  }]
  "web_accessible_resources": [
    "JokLogo.png"
  ],
  "manifest_version": 4
}
