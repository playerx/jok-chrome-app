{
  "name": "Jok",
  "description": "It's jok time :)",
  "version": "1.4",
  "default_locale": "en",
  "app": {
    "launch": {
      "local_path": "index.html",
      "container":"tab"
    }
  },
  "icons": {
    "128": "JokLogo.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ],
  "content_scripts" : [{
    "matches" : [ "https://*/"],
    "js" : ["script.js"]
  }],
  "web_accessible_resources": [
    "JokLogo.png"
  ],
  "manifest_version": 4
}
